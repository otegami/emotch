<%= form_with model: @tweet, local: :true do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :content, placeholder: "25words change your world"%>
  </div>
  <div class="file">
    <%= f.file_field :image %>
  </div>
  <%= f.submit "Post", class: "" %>
<% end %>
